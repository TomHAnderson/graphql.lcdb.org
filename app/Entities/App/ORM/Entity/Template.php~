<?php

namespace App\ORM\Entity;

/**
 * Template
 */
class Template
{
    /**
     * @var string
     */
    private $name = '';

    /**
     * @var string
     */
    private $author = '';

    /**
     * @var string|null
     */
    private $description;

    /**
     * @var string|null
     */
    private $default_colors;

    /**
     * @var string|null
     */
    private $default_columns;

    /**
     * @var string|null
     */
    private $longdesc;

    /**
     * @var bool
     */
    private $smarty = '0';

    /**
     * @var bool
     */
    private $forcelist = '0';

    /**
     * @var bool
     */
    private $zendview = '0';

    /**
     * @var int
     */
    private $id;

    /**
     * @var \Doctrine\Common\Collections\Collection
     */
    private $users;

    /**
     * @var \Doctrine\Common\Collections\Collection
     */
    private $userLists;

    /**
     * Constructor
     */
    public function __construct()
    {
        $this->users = new \Doctrine\Common\Collections\ArrayCollection();
        $this->userLists = new \Doctrine\Common\Collections\ArrayCollection();
    }

}
